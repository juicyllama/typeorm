{
  "semi": false,
  "singleQuote": true,
  "trailingComma": "es5",
  "endOfLine": "auto",
  "useTabs": false,
  "tabWidth": 4,
  "printWidth": 120,
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "bracketSameLine": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrderParserPlugins": ["typescript", "decorators-legacy"],
  "importOrder": [
    "^@?\\w",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
